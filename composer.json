{
    "name": "jungi/framework-extra-bundle",
    "description": "attributes and annotations for request and response operations, content negotiation, and more for symfony projects",
    "keywords": ["body", "param", "conversion", "mapper"],
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.2.9",
        "symfony/framework-bundle": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symfony/dependency-injection": "^5.3"
    },
    "suggest": {
        "symfony/serializer": "For serializer adapters",
        "doctrine/annotations": "For annotations"
    },
    "require-dev": {
        "symfony/serializer": "^5.3",
        "symfony/property-info": "^5.3",
        "symfony/phpunit-bridge": "^5.3",
        "doctrine/annotations": "^1.6"
    },
    "authors": [
        {
            "name": "Piotr Kugla",
            "email": "piku235@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jungi\\FrameworkExtraBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jungi\\FrameworkExtraBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
